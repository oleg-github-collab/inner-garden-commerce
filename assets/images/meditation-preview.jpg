<svg width="600" height="400" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="medGrad1" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#667eea;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#764ba2;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#667eea;stop-opacity:0.8" />
    </linearGradient>
    <radialGradient id="medGrad2" cx="50%" cy="50%" r="50%">
      <stop offset="0%" style="stop-color:#ffffff;stop-opacity:0.3" />
      <stop offset="100%" style="stop-color:#667eea;stop-opacity:0.1" />
    </radialGradient>
  </defs>
  
  <!-- Background -->
  <rect width="100%" height="100%" fill="url(#medGrad1)"/>
  
  <!-- Meditation circles -->
  <circle cx="300" cy="200" r="120" fill="url(#medGrad2)" opacity="0.6"/>
  <circle cx="300" cy="200" r="80" fill="url(#medGrad2)" opacity="0.4"/>
  <circle cx="300" cy="200" r="40" fill="url(#medGrad2)" opacity="0.8"/>
  
  <!-- Decorative elements -->
  <circle cx="150" cy="100" r="20" fill="rgba(255,255,255,0.3)"/>
  <circle cx="450" cy="320" r="25" fill="rgba(255,255,255,0.2)"/>
  <circle cx="100" cy="300" r="15" fill="rgba(255,255,255,0.4)"/>
  <circle cx="500" cy="80" r="18" fill="rgba(255,255,255,0.3)"/>
  
  <!-- Center meditation symbol -->
  <circle cx="300" cy="200" r="8" fill="white" opacity="0.9"/>
  
  <!-- Text -->
  <text x="300" y="350" text-anchor="middle" fill="white" font-family="Arial, sans-serif" font-size="24" font-weight="500" opacity="0.9">Meditation Journey</text>
</svg>